echo ${0##*/}: begin
echo stream addres from cliipboard to s
echo mouse net zapasy wstilka shtany shorty tb wuha rukavitsi
mem &
watchclip chaturbate | while read r
do
	echo ${0##*/}: get $r

	#age
	lynx --dump $r | grep -C4 Age:

	#save image to shm
	cd /dev/shm
	echo $r | sed -e s/chaturbate.com/roomimg.stream.highwebmedia.com\\/ri/g |sed -e s/\$/.jpg/g |sed -e s/\\/.jpg/.jpg/g | xargs wget -q &

	#launch
	s $r
done

