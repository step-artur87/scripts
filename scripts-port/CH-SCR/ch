watchclip chaturbate | while read r
do
	echo get $r
	open image in browser
	echo $r | sed -e s/chaturbate.com/roomimg.stream.highwebmedia.com\\/ri/g |sed -e s/\$/.jpg/g |sed -e s/\\/.jpg/.jpg/g | xargs netsurf-gtk &
	mousepad $r
	echo $r >> /mnt/sda2/files/chlog
	a=$(date +"%s")
	streamlink $r 240p,480p,576p,600p,720p --player "mpv  --no-audio --force-media-title $r" -Q
	b=$(date +"%s")
	echo $(( $b - $a ))
	echo $(( $b - $a )) >> /mnt/sda2/files/chlog
done

